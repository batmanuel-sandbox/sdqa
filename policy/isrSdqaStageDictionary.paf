#<?cfg paf dictionary ?>
#
# Dictionary for IsrSdqaStage policies
#

target: lsst.sdqa.IsrSdqaStage

definitions: {
    #input clipboard keys
    inputKeys: {
        type: "policy"
        dictionary: {
            definitions: {
                exposureKey: {
                    type: "string"
                    description: "Name of clipboard keyword for exposure."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "isrExposure"
                }        
            }
        }
        maxOccurs: 1        
    }
    
    #output clipboard keys:
    outputKeys: {
        type: "policy"
        dictionary: {
            definitions: {
                isrPersistableSdqaRatingVectorKey: {
                    type: "string"
                    description: "Name of clipboard keyword for persistable SDQA rating vector for IRS pipeline."
                    maxOccurs: 1
                    default: "isrPersistableSdqaRatingVector"
                }
            }
        }
        maxOccurs: 1
    }

    parameters: {
        type: policy
        dictionary: {
            target: Parameters
            definitions: {
                sdqaRatingScope: {
                    type: int
                    default: 0
                    minOccurs: 1
                    maxOccurs: 1
                    allowed: {
                        min: 0
                        max: 3
                    }
                    description:  "The scope of the set of SDQA ratings 
                                   associated with this stage instance:
                                   0=AMP, 1=CCD, 2=FPA, 3=FOOTPRINT"
                }
                nBadCalibPixKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating nBadCalibPix."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "nBadCalibPix"
                }
                nSaturatePixKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating nSaturatePix."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "nSaturatePix"
                }
                overscanMeanKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating overscanMean."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "overscanMean"
                }
                overscanMeanUncKey: {
                    type: "string"
                    description: "Name of metadata keyword for uncertainty of SDQA rating overscanMean."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "overscanMeanUnc"
                }
                overscanStdDevKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating overscanStdDev."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "overscanStdDev"
                }
                overscanMedianKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating overscanMedian."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "overscanMedian"
                }
                imageStdDevKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageStdDev."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageStdDev"
                }
                 imageClippedMeanKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageClippedMean."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageClippedMean4Sigma3Passes"
                }
                imageMedianKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageMedian."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageMedian"
                }
                imageMinKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageMin."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageMin"
                }
                imageMaxKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageMax."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageMax"
                }
                xImageGradientKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageGradientX."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageGradientX"
                }
                yImageGradientKey: {
                    type: "string"
                    description: "Name of metadata keyword for SDQA rating imageGradientY."
                    minOccurs: 1
                    maxOccurs: 1
                    default: "imageGradientY"
                }
            }
        }
    }
}
