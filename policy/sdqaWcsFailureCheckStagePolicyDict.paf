#<?cfg paf dictionary ?>  
# 
# Policy dictionary for SdqaWcsFailureCheckStage.  
# 
target:  SdqaWcsCheckFailureStage
definitions: {
    inputKeys: {
        type: policy
        dictionary: {
            target: Parameters
            definitions: {
                ccdMetadataKey: {
                    type: string
                    description: "name of clipboard keyword for CCD metadata."
                    minOccurs: 1
                    maxOccurs: 1
                    default: ccdMetadataKey
                }
                extractedSourceSetKey: {
                    type: string
                    description: "name of clipboard keyword for set of extracted sources 
                                  from CCD image."
                    minOccurs: 1
                    maxOccurs: 1
                    default: extractedSourceSetKey
                }
            }
        }
    }
    outputKeys: {
        type: policy
        dictionary: {
            target: Parameters
            definitions: {
                astromVerifSdqaRatingsKey: {
                    type: string
                    description: "name of clipboard keyword for set of SDQA ratings 
                                  associated with WCS verification."
                    minOccurs: 1
                    maxOccurs: 1
                    default: astromVerifSdqaRatingsKey
                }
            }
        }
    }
    parameters: {
        type: policy
        dictionary: {
            target: Parameters
            definitions: {
            minMatches: {
                type: int
                default: 20
                minOccurs: 1
                maxOccurs: 1
                allowed: {
                    min: 10
                    max: 100
                }
                description:  "the minimum number of matches between extracted sources
                               and astrometric-reference sources."
            }
            maxRmsRadDist: {
                type:  double
                default:  1.5
                minOccurs: 1
                maxOccurs:  1
                allowed: {
                    min: 0.1
                    max: 5.0
                }
                description:  "the maximum allowed root-mean-squared radial distance
                               computed for matches between extracted sources and
                               astrometric-reference sources (arcseconds)."
                }
                matchRadius: {
                    type:  double
                    default:  5.0
                    minOccurs: 1
                    maxOccurs:  1
                    allowed: {
                        min: 0.1
                        max: 10.0
                    }
                    description:  "the maximum allowed radius for matches between extracted 
                                   sources and astrometric-reference sources (arcseconds)."
                }
                minStellarity: {
                    type:  double
                    default:  0.85
                    minOccurs: 1
                    maxOccurs:  1
                    allowed: {
                        min: 0.5
                        max: 0.95
                    }
                    description:  "the minimum stellarity for selection of 
                                   extracted sources (dimensionless)."
                }
                nLocalBkgSigma: {
                    type:  int
                    default:  4
                    minOccurs:  1
                    maxOccurs:  1
                    allowed: {
                        min: 3
                        max: 6
                    }
                    description:  "number of standard deviations above 
                                   local background required of peak pixel
                                   in source extraction, for selection of 
                                   extracted sources (dimensionless)."
                }
            }
        }
    }
}
